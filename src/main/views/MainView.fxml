<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.controllers.MainController">

    <HBox>
        <Button fx:id="selectFileButton" onAction="#bttFileClicked" text="Select Reference Image">
         <HBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </HBox.margin></Button>
        <Button fx:id="selectDirectoryButton" onAction="#bttDirectoryClicked" text="Select Comparison Directory">
         <HBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </HBox.margin></Button>
        <Button fx:id="computeButton" maxWidth="1.7976931348623157E308" onAction="#bttComputeClicked" text="Compute Data">
         <HBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </HBox.margin></Button>
    </HBox>

    <fx:include fx:id="resultView" source="ResultView.fxml" />
    
</VBox>
